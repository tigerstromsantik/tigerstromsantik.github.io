---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "categories": "{{ post.categories | array_to_sentence_string }}",
			"theme": "{{ post.theme }}",
			"image": "{{ post.image }}",
      "url": "{{ post.url }}",
      "date": "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
